// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["public", "cor"]
}

model SystemHealth {
  id       Int     @id @default(autoincrement())
  isHealth Boolean @default(true)

  @@map("system_health") 
  @@schema("cor")      
}

